    onStartEvent: []
    onCompleteEvent: []
  - conversationPointSpeaker: {fileID: 11400000, guid: 7d3ea8f2dae75fe43a90f89bb2628475,
      type: 2}
    conversationPointText: 'I go where I want. '
    typingDelay: 0
    onStartEvent: []
    onCompleteEvent: []
  - conversationPointSpeaker: {fileID: 11400000, guid: bfa3c61bf95f5ba4c858a4d07c0d30bb,
      type: 2}
    conversationPointText: 'Oh. Fair enough. '
    typingDelay: 0.03
