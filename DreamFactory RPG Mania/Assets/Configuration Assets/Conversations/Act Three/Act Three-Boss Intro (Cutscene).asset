    onCompleteEvent: []
  - conversationPointSpeaker: {fileID: 11400000, guid: 3f5d4d8dd6308b442a5a35db8da9efb3,
      type: 2}
    conversationPointText: 'We should get back to the other workers. '
    typingDelay: 0.03
    onStartEvent: []
    onCompleteEvent: []
  - conversationPointSpeaker: {fileID: 11400000, guid: f4bdd2adbdb296148b226e7615591cf5,
      type: 2}
    conversationPointText: 'Puffer! Great job, kid. The factory seems to be all right. '
    typingDelay: 0.03
    onStartEvent: []
    onCompleteEvent: []
  - conversationPointSpeaker: {fileID: 11400000, guid: d56315340d38a9a40a214f38f6140890,
      type: 2}
    conversationPointText: Hahahahaha! Don't speak too soon.
    typingDelay: 0.03
    onStartEvent: []
    onCompleteEvent: []
  - conversationPointSpeaker: {fileID: 11400000, guid: 3f5d4d8dd6308b442a5a35db8da9efb3,
      type: 2}
    conversationPointText: 'Who was that? '
    typingDelay: 0.03
    onStartEvent: []
    onCompleteEvent: []
  - conversationPointSpeaker: {fileID: 11400000, guid: 7d3ea8f2dae75fe43a90f89bb2628475,
      type: 2}
    conversationPointText: Oh, no. I think I know who that is.
    typingDelay: 0.03
